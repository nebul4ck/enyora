[database]
enyora_db = 'testdb.db'
enyora_table = 'enyora_registry'

[sql]
sql_enyora_table = "CREATE TABLE IF NOT EXISTS %s (
                    id integer PRIMARY KEY,
                    reg_date text NOT NULL,
                    reg_hour text NOT NULL,
                    action text NOT NULL,
                    work_diff text,
                    work_total text
                );"

sql_last_row = "SELECT rowid,action,reg_hour from %s 
                    WHERE reg_date LIKE '%s' ORDER BY 
                    rowid DESC LIMIT 1;"

sql_query_in = "INSERT INTO %s 
				(reg_date, reg_hour, action, work_diff, work_total) 
				VALUES ('%s','%s','%s','%s','%s')"
